{
  "ConnectionStrings": {
    "Identity": "Server=(localdb)\\mssqllocaldb;Database=JF;Trusted_Connection=True;"
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "Service": {
    "Name": "CSYS.Identity",
    "NamingService": "http://localhost:64472"
  },
  "Identity": {
    "ClaimsIdentity": {
      "AuthSchema": "Jwt.Accese.Token",
      // "EmailClaimType": "Email",
      // "RoleClaimType": "Role",
      "SecurityStampClaimType": "SecurityStamp"
      // "UserIdClaimType": "Id"
    },
    "Lockout": {
      "AllowedForNewUsers": true,
      "DefaultLockoutTimeSpan": "0:20",
      "MaxFailedAccessAttempts": 20
    },
    "SignIn": {
      "AccessToken": {
        "Audience": "hjpt.identity",
        "Expiration": "0:20",
        "Issuer": "hjpt.auth.access"
      },
      "RefreshToken": {
        "Audience": "hjpt.identity",
        "Expiration": "365.0:0:0",
        "Issuer": "hjpt.auth.refresh"
      },
      "RequireConfirmedEmail": false
    },
    "SignUp": {

    }
  }
}
